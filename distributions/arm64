# Debian

[unstable]
suite: unstable
mirror: http://ftp.debian.org/debian
components: main contrib non-free
extramirrors: deb http://debomatic-arm64.debian.net/debomatic/unstable unstable main

[experimental]
suite: unstable
mirror: http://ftp.debian.org/debian
components: main contrib non-free
extramirrors: deb http://ftp.debian.org/debian/ experimental main contrib non-free
              deb http://debomatic-arm64.debian.net/debomatic/experimental experimental main

[testing]
suite: testing
mirror: http://ftp.debian.org/debian
components: main contrib non-free

[stable]
suite: stable
mirror: http://ftp.debian.org/debian
components: main contrib non-free
extramirrors: deb http://security.debian.org/ stable/updates main contrib non-free
              deb http://ftp.debian.org/debian stable-updates main contrib non-free

[oldstable]
suite: oldstable
mirror: http://ftp.debian.org/debian
components: main contrib non-free
extramirrors: deb http://security.debian.org/ oldstable/updates main contrib non-free
              deb http://ftp.debian.org/debian oldstable-updates main contrib non-free

[jessie-backports]
suite: jessie
mirror: http://ftp.debian.org/debian
