# Debian

[unstable]
suite: unstable
mirror: http://ftp.debian.org/debian
components: main contrib non-free
extramirrors: deb http://debomatic-s390x.debian.net/debomatic/unstable unstable main

[experimental]
suite: unstable
mirror: http://ftp.debian.org/debian
components: main contrib non-free
extramirrors: deb http://ftp.debian.org/debian/ experimental main contrib non-free
              deb http://debomatic-s390x.debian.net/debomatic/experimental experimental main

[testing]
suite: testing
mirror: http://ftp.debian.org/debian
components: main contrib non-free

[stable]
suite: stable
mirror: http://ftp.debian.org/debian
components: main contrib non-free
extramirrors: deb http://security.debian.org/ stable/updates main contrib non-free
              deb http://ftp.debian.org/debian stable-updates main contrib non-free

[oldstable]
suite: oldstable
mirror: http://ftp.debian.org/debian
components: main contrib non-free
extramirrors: deb http://security.debian.org/ oldstable/updates main contrib non-free
              deb http://ftp.debian.org/debian oldstable-updates main contrib non-free

[jessie-backports]
suite: jessie
mirror: http://ftp.debian.org/debian
components: main contrib non-free
extramirrors: deb http://security.debian.org/ jessie/updates main contrib non-free
              deb http://ftp.debian.org/debian jessie-updates main contrib non-free
              deb http://ftp.debian.org/debian jessie-backports main contrib non-free

[wheezy-backports]
suite: wheezy
mirror: http://ftp.debian.org/debian
components: main contrib non-free
extramirrors: deb http://security.debian.org/ wheezy/updates main contrib non-free
              deb http://ftp.debian.org/debian wheezy-updates main contrib non-free
              deb http://ftp.debian.org/debian wheezy-backports main contrib non-free



# Ubuntu

[yakkety]
suite: yakkety
mirror: http://ports.ubuntu.com
components: main restricted universe multiverse
extramirrors: deb http://ports.ubuntu.com yakkety-updates main restricted universe multiverse
              deb http://ports.ubuntu.com yakkety-security main restricted universe multiverse
              deb http://debomatic-powerpc.debian.net/debomatic/yakkety yakkety main
extrapackages: pkgbinarymangler pkg-create-dbgsym

[xenial]
suite: xenial
mirror: http://ports.ubuntu.com
components: main restricted universe multiverse
extramirrors: deb http://ports.ubuntu.com xenial-updates main restricted universe multiverse
              deb http://ports.ubuntu.com xenial-security main restricted universe multiverse
              deb http://debomatic-powerpc.debian.net/debomatic/xenial xenial main
